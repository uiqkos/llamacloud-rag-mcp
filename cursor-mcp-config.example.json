      "command": "node",
      "args": ["/absolute/path/to/llamacloud-mcp-new/dist/index.js"],
      "env": {
        "LLAMA_CLOUD_API_KEY": "your-llamacloud-api-key-here",
        "LLAMA_CLOUD_ORGANIZATION_ID": "your-organization-id",
